	<subDataset name="dataset1" uuid="c19a8940-5445-4aac-95be-401c7d870329">
		<parameter name="periods_iso" class="java.lang.String"/>
		<parameter name="organisationunits_uid" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT COUNT(DISTINCT ppr.patient_id) AS Total
FROM isanteplus.patient_pregnancy ppr JOIN isanteplus.patient p ON ppr.patient_id = p.patient_id,
	isanteplus.patient_laboratory plab, isanteplus.org_code_uid org
WHERE ppr.patient_id=plab.patient_id
	AND p.organisation_uid = org.uid
	AND FIND_IN_SET($P{organisationunits_uid}, org.path)
	AND (plab.test_id=1042 OR plab.test_id=1040)
	AND plab.test_result IN (664,703,1138,1304)
	AND ((DATE_FORMAT(ppr.start_date, "%Y%m") = $P{periods_iso}) OR (DATE_FORMAT(ppr.end_date, "%Y%m") = $P{periods_iso}))
	AND DATE_FORMAT(plab.date_test_done, "%Y%m") = $P{periods_iso};]]>
		</queryString>
		<field name="Total" class="java.lang.Long">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
	</subDataset>
