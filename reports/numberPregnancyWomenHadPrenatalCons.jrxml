		<parameter name="organisationunits_uid" class="java.lang.String">
			<defaultValueExpression><![CDATA["a7SxSzggwzc"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select count(distinct vtype.patient_id) as Total
			FROM isanteplus.visit_type vtype
			LEFT OUTER JOIN isanteplus.patient p
			ON p.patient_id=vtype.patient_id,
			isanteplus.patient_pregnancy pp, isanteplus.org_code_uid org,
			(select vt.patient_id, MIN(vt.encounter_date) as encounter_date
			FROM isanteplus.visit_type vt WHERE vt.concept_id=160288 AND vt.v_type=1622 GROUP BY 1) B
			WHERE vtype.patient_id=pp.patient_id
			AND vtype.concept_id=160288 AND vtype.v_type=1622
			AND vtype.patient_id = B.patient_id
			AND vtype.encounter_date = B.encounter_date
			AND DATE_FORMAT(vtype.encounter_date, "%Y%m") = $P{periods_iso}
			AND DATE_FORMAT(pp.start_date, "%Y%m") = $P{periods_iso}
			AND p.organisation_uid = org.uid
			AND FIND_IN_SET($P{organisationunits_uid}, org.path)]]>
		</queryString>
		<field name="Total" class="java.lang.Long"/>
	</subDataset>
