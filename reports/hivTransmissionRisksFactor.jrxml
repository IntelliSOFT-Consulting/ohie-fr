	<subDataset name="dataset1" uuid="c19a8940-5445-4aac-95be-401c7d870329">
		<parameter name="periods_iso" class="java.lang.String"/>
		<parameter name="organisationunits_uid" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT cn.name AS Mode, COUNT(distinct vrf.patient_id) as Total
	FROM openmrs.concept_name cn, isanteplus.vih_risk_factor vrf, isanteplus.patient p, isanteplus.org_code_uid org
WHERE cn.concept_id=vrf.risk_factor
	AND vrf.patient_id = p.patient_id
    AND p.organisation_uid = org.uid
	AND cn.locale='fr'
	AND DATE_FORMAT(vrf.encounter_date, "%Y%m") = $P{periods_iso}
	AND FIND_IN_SET($P{organisationunits_uid} , org.path)
GROUP BY cn.name;]]>
		</queryString>
		<field name="Mode" class="java.lang.String"/>
		<field name="Total" class="java.lang.Long">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
	</subDataset>
