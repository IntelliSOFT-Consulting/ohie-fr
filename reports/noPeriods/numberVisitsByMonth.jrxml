	<subDataset name="dataset1" uuid="c19a8940-5445-4aac-95be-401c7d870329">
		<parameter name="organisationunits_uid" class="java.lang.String">
			<defaultValueExpression><![CDATA["a7SxSzggwzc"]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT DISTINCT loc.name AS Site,
	MONTH(tab.encounter_datetime) AS Month,
	YEAR(tab.encounter_datetime) AS Year,
	COUNT(DISTINCT tab.patient_id) AS Count
		FROM openmrs.location loc, isanteplus.org_code_uid org,
			(SELECT en.encounter_datetime, en.patient_id, en.location_id, organisation_uid
				FROM isanteplus.patient p, openmrs.encounter en
					WHERE p.patient_id = en.patient_id) AS tab
			WHERE tab.location_id=loc.location_id
                			AND tab.organisation_uid = org.uid
				AND FIND_IN_SET($P{organisationunits_uid}, org.path)
			GROUP BY loc.name, MONTH(tab.encounter_datetime), YEAR(tab.encounter_datetime)
			ORDER BY 3 DESC;]]>
		</queryString>
		<field name="Site" class="java.lang.String"/>
		<field name="Month" class="java.lang.Integer"/>
		<field name="Year" class="java.lang.Integer"/>
		<field name="Count" class="java.lang.Long"/>
	</subDataset>
