	<subDataset name="dataset1" uuid="c19a8940-5445-4aac-95be-401c7d870329">
		<parameter name="organisationunits_uid" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT cn.name AS 'Nom du médicament', COUNT(DISTINCT pp.patient_id) AS Quantité
FROM openmrs.concept_name cn,isanteplus.patient_prescription pp, isanteplus.patient p, isanteplus.org_code_uid org
WHERE cn.concept_id=pp.drug_id
	AND cn.locale='fr'
	AND pp.patient_id = p.patient_id
	AND p.organisation_uid = org.uid
	AND FIND_IN_SET($P{organisationunits_uid}, org.path)
GROUP BY cn.name;]]>
		</queryString>
		<field name="Nom du médicament" class="java.lang.String"/>
		<field name="Quantité" class="java.lang.Long"/>
	</subDataset>
