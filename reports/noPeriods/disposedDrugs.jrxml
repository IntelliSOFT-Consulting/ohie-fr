	<subDataset name="dataset1" uuid="c19a8940-5445-4aac-95be-401c7d870329">
		<parameter name="organisationunits_uid" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT cn.name AS MedicationName, COUNT(DISTINCT pdis.patient_id) AS Count
FROM openmrs.concept_name cn, isanteplus.patient_dispensing pdis, isanteplus.patient p, isanteplus.org_code_uid org
WHERE cn.concept_id = pdis.drug_id
	AND cn.locale='fr'
    	AND pdis.patient_id = p.patient_id
	AND p.organisation_uid = org.uid
	AND FIND_IN_SET($P{organisationunits_uid}, org.path)
GROUP BY cn.name;]]>
		</queryString>
		<field name="MedicationName" class="java.lang.String"/>
		<field name="Count" class="java.lang.Long"/>
	</subDataset>
	<parameter name="organisationunits_uid" class="java.lang.String"/>
