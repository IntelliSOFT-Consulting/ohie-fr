		<queryString>
			<![CDATA[SELECT COUNT(DISTINCT pv.patient_id) AS Total
FROM isanteplus.org_code_uid org, isanteplus.pediatric_hiv_visit pv
INNER JOIN isanteplus.patient p
ON p.patient_id=pv.patient_id
WHERE pv.actual_vih_status=1405
      AND pv.ptme=1065
      AND DATE_FORMAT(pv.encounter_date, "%Y%m") = $P{periods_iso}
      AND p.organisation_uid = org.uid
      AND FIND_IN_SET($P{organisationunits_uid}, org.path);]]>
		</queryString>
		<field name="Total" class="java.lang.Long"/>
	</subDataset>
